
@{
    ViewData["Title"] = "Docupages";
}
<link rel="stylesheet" href="~/css/Docupages.css" asp-append-version="true" />
<script src="https://kit.fontawesome.com/9630638ea4.js" crossorigin="anonymous"></script>


<div class="pdfcontainer">
    <div class="pdflist">
     
        <section>
            
            <iframe class="pdfiframe" src="~/docu/pdf/notes1.pdf"  frameborder="0"></iframe>
        </section>

    </div>


</div>
<div class="songbanner"></div>

<div class="bottom">
    <input type="range" name="range" id="myprogressBar" min="0" value="0" max="100">
    <div class="icon">
        <i class="fas fa-2x fa-backward" id="backward"></i>
        <i class="far fa-2x fa-play-circle" id="masterplay"></i>
        <i class="fas fa-2x fa-forward" id="forward"></i>
    </div>
    <div class="songInfo">
        <img src="playing.gif" width="42px" id="gif"><span id="mastersongname">A-Salam</span>
    </div>
</div>